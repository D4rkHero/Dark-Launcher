<Application x:Class="Dark_Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dark_Launcher"
             StartupUri="View/Launcher.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/History/CloseButton.xaml" />
                <ResourceDictionary Source="Themes/History/Progressbar.xaml" />
                <ResourceDictionary Source="Themes/History/Text.xaml"/>
                <ResourceDictionary Source="Themes/History/PlayButton.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
