<Window x:Class="Dark_Launcher.View.Splash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dark_Launcher.View"
        mc:Ignorable="d"
        AllowsTransparency="True"
        WindowStyle="None"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        ContentRendered="OnSplashContentRendered"
        Title="Splash" Height="200" Width="290">

    <Window.Background>
        <ImageBrush ImageSource="pack://application:,,,/Resources/Images/logo.jpg"/>
    </Window.Background>
    <Grid>
        
        
    </Grid>
</Window>
